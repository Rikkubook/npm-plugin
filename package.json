{
  "name": "rikku-plugin",
  "version": "0.0.3",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "vue-tsc && vite build",
    "preview": "vite preview"
  },
  "files": ["dist"],
  "main": "./dist/ri-ui.umd.js",
  "module": "./dist/ri-ui.es.js",
